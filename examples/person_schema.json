{
    "$id": "http://example.com/person.json",
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "Person",
    "type": "object",
    "required": [ "name", "age" ],
    "properties": {
        "name": {
            "type": "string",
            "default": "John Doe"
        },
        "age": {
            "type": "integer",
            "default": 30
        }
    }
}
